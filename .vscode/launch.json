	"version": "0.2.0",
  "configurations": [
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--folder-uri=${workspaceFolder}/tests",
        "${workspaceFolder}/tests/google.py",
        "${workspaceFolder}/tests/numpy.py",
        "${workspaceFolder}/tests/sphinx.py",
      ]
    }
  ]
}
